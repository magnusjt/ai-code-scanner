{
  "compilerOptions": {
    "lib": ["esnext"],
    "module": "commonjs",
    "target": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "strictNullChecks": true,
  }
}